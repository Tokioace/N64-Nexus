{
  "name": "battle64-eventsystem",
  "version": "1.0.0",
  "description": "Event system for N64 speedruns and time trials",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.2",
    "sharp": "^0.32.5",
    "tesseract.js": "^4.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["n64", "speedrun", "events", "gaming", "battle64"],
  "author": "Battle64 Team",
  "license": "MIT"
}